{
  "name": "shared-npm-publish",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/shared/npm-publish/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/shared/npm-publish",
        "tsConfig": "packages/shared/npm-publish/tsconfig.lib.json",
        "packageJson": "packages/shared/npm-publish/package.json",
        "main": "packages/shared/npm-publish/src/index.ts",
        "assets": ["packages/shared/npm-publish/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/shared/npm-publish/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/shared/npm-publish/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "exec": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "nx run shared-npm-publish:build",
            "forwardAllArgs": false
          },
          {
            "command": "node dist/packages/shared/npm-publish/src/index.js",
            "forwardAllArgs": true
          }
        ]
      }
    }
  },
  "tags": []
}
